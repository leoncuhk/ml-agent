# ML-Agent 环境配置文件
# 请将此文件复制为 .env 并填入您的实际配置

# OpenAI API 配置 (必需)
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_API_KEY=your_openai_api_key_here

# 或者使用其他兼容的API服务
# OPENAI_API_BASE=https://your-custom-api-endpoint.com/v1
# OPENAI_API_KEY=your_custom_api_key

# H2O AutoML 配置 (可选)
H2O_PORT=54321
H2O_MAX_MEM_SIZE=4g

# 日志配置 (可选)
LOG_LEVEL=INFO

# 模型保存配置 (可选)
MODEL_SAVE_PATH=models/
LOG_SAVE_PATH=logs/
RESULTS_SAVE_PATH=results/

# Web应用配置 (可选)
FLASK_HOST=0.0.0.0
FLASK_PORT=8000
FLASK_DEBUG=False 